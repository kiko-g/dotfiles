{
  //@@ Languages Behavior
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 110
  },
  "[mdx]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.wordWrap": "on"
  },
  "[jsonc]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.tabSize": 2,
    "editor.suggest.insertMode": "replace",
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[java]": {
    "editor.tabSize": 4
  },
  "[javascript]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.tabSize": 2
  },
  "[css]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[php]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client",
    "editor.formatOnSave": false
  },
  "[blade]": {
    "editor.autoClosingBrackets": "always",
    "editor.defaultFormatter": "shufo.vscode-blade-formatter",
    "editor.formatOnSave": false
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "postcss": "css"
  },
  "emmet.syntaxProfiles": {
    "javascript": "jsx",
    "postcss": "css"
  },
    "prettier.tabWidth": 2,
  "prettier.printWidth": 100,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "python.defaultInterpreterPath": "/usr/local/bin/python3.9",
  
  // @@ Highlighting
  "highlight-matching-tag.styles": {
    //opening tag
    "closing": {
      "full": { "highlight": "", "surround": "", "underline": "" },
      "left": { "highlight": "", "surround": "", "underline": "" },
      "name": { "highlight": "#d1ffe650", "surround": "", "underline": "#d1ffe690" },
      "right": { "highlight": "", "surround": "", "underline": "" }
    },
    //opening tag
    "opening": {
      "full": { "highlight": "", "surround": "", "underline": "" },
      "left": { "highlight": "", "surround": "", "underline": "" },
      "name": { "highlight": "#d1ffe650", "surround": "", "underline": "#d1ffe690" },
      "right": { "highlight": "", "surround": "", "underline": "" }
    },
    //inner context of tag pair
    "inner": {
      "highlight": "",
      "surround": "",
      "underline": ""
    }
  },
  
  "todohighlight.keywords": [
    {
      "text": "@@",
      "color": "#ffffff",
      "backgroundColor": "#E5C07B70",
      "overviewRulerColor": "#E5C07B40",
      "isWholeLine": true,
    },
    {
      "text": "!!!",
      "color": "#ffffff",
      "backgroundColor": "#61AFEF70",
      "overviewRulerColor": "#61AFEF40",
      "isWholeLine": true
    }
  ],
  
