{
  /* @@@ Editor Behavior */
  "editor.fontSize": 12.5,
  "editor.wordWrapColumn": 100,
  "editor.rulers": [105],
  "editor.fontFamily": "JetBrains Mono, monospace",
  "editor.cursorBlinking": "phase",
  "editor.suggestSelection": "first",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.quickSuggestionsDelay": 10,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.fontLigatures": false,
  "editor.minimap.enabled": false,
  "editor.overviewRulerBorder": true,
  "editor.cursorSmoothCaretAnimation": true,
  "editor.acceptSuggestionOnCommitCharacter": true,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.suggestOnTriggerCharacters": true,
  "editor.tabCompletion": "off",
  "editor.suggest.localityBonus": true,
  "editor.wordBasedSuggestions": false,
  "editor.parameterHints.enabled": true,
  /* Format */
  "editor.formatOnType": false,
  "editor.formatOnSave": false,
  "editor.formatOnPaste": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2,
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "editor.linkedEditing": true,
  "workbench.tree.indent": 5,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.startupEditor": "newUntitledFile",
  "explorer.sortOrder": "type",

  /*  */
  "prettier.semi": false,
  "prettier.tabWidth": 2,
  "prettier.printWidth": 120,

  /* @@@ Highlighting Keywords */
  "todohighlight.keywords": [
    {
      "text": "@@@",
      "color": "white",
      "backgroundColor": "#2f68aa",
      "isWholeLine": false,
      "overviewRulerColor": "#FFFFFF22"
    },
    {
      "text": "~~~",
      "color": "white",
      "backgroundColor": "#43a58e",
      "isWholeLine": false,
      "overviewRulerColor": "#FFFFFF00"
    },
    {
      "text": "+++",
      "color": "#ffffff",
      "backgroundColor": "#c85454",
      "isWholeLine": false,
      "overviewRulerColor": "#FFFFFF00"
    },
    {
      "text": "************",
      "color": "#ffffff",
      "backgroundColor": "#ff9e7433",
      "isWholeLine": true,
      "overviewRulerColor": "#FFFFFF00"
    }
  ],

  /* @@@ Matching Tags */
  "highlight-matching-tag.styles": {
    "closing": {
      "full": {
        "highlight": "",
        "surround": "",
        "underline": ""
      },
      "left": {
        "highlight": "",
        "surround": "",
        "underline": ""
      },
      "name": {
        "highlight": "#d1ffe650",
        "surround": "",
        "underline": "#d1ffe690"
      },
      "right": {
        "highlight": "",
        "surround": "",
        "underline": ""
      }
    },
    "opening": {
      "full": {
        "highlight": "",
        "surround": "",
        "underline": ""
      },
      "left": {
        "highlight": "",
        "surround": "",
        "underline": ""
      },
      "name": {
        "highlight": "#d1ffe650",
        "surround": "",
        "underline": "#d1ffe690"
      },
      "right": {
        "highlight": "",
        "surround": "",
        "underline": ""
      }
    },
    "inner": {
      "highlight": "",
      "surround": "",
      "underline": ""
    }
  },

  /* @@@ Languages */
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[env]": {
    "editor.defaultFormatter": "IronGeek.vscode-env"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[javascript]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.tabSize": 4,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 120,
    "editor.formatOnSave": false
  },
  "[dart]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "Dart-Code.dart-code"
  },
  "[svg]": {
    "editor.defaultFormatter": "jock.svg"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "css.validate": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client",
    "editor.formatOnSave": true
  },
  "[blade]": {
    "editor.autoClosingBrackets": "always",
    "editor.formatOnSave": true
  },
  "[haskell]": {
    "editor.bracketPairColorization.enabled": true
  },
  "[cpp]": {
    "editor.tabSize": 4
  },
  "[prolog]": {
    "editor.formatOnType": false,
    "editor.formatOnSave": false,
    "editor.formatOnPaste": false
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "postcss": "css"
  },
  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",

