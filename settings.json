  },

  /* @@@ Editor Colors */
  "workbench.colorTheme": "Tailwind Moon", // One Monokai, Atom One Dark, New Moon
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Boolean",
        "scope": "punctuation.definition.template-expression",
        "settings": {
          "foreground": "#F472B6" //ff635e ff9a9a
        }
      },
      {
        "name": "Boolean",
        "scope": "constant.language",
        "settings": {
          "foreground": "#ff7777" //ff635e ff9a9a
        }
      },
      {
        "name": "Parameter variables",
        "scope": ["variable.other.constant.property"],
        "settings": {
          "fontStyle": "",
          "foreground": "#60A5FA"
        }
      },
      {
        "name": "Parameter variables",
        "scope": ["variable.parameter"],
        "settings": {
          "fontStyle": "bold",
          "foreground": "#ffffff"
        }
      },
      {
        "name": "Markdown Bold",
        "scope": ["markup.bold.markdown"],
        "settings": {
          "fontStyle": "bold",
          "foreground": "#3286da"
        }
      },
      {
        "name": "Markdown Italic",
        "scope": ["markup.italic.markdown"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#87c3ff"
        }
      },
      {
        "name": "Markdown Bullet",
        "scope": ["punctuation.definition.list.begin.markdown"],
        "settings": {
          "fontStyle": "bold",
          "foreground": "#A78BFA"
        }
      },
      {
        "name": "Markdown Quote",
        "scope": ["markup.quote.markdown"],
        "settings": {
          "fontStyle": "",
          "foreground": "#34D399"
        }
      }
    ]
  },

  /* @@@ File/folder settings */
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.activeIconPack": "react",
  "material-icon-theme.saturation": 1,
  "material-icon-theme.folders.color": "#aaccee",
  "material-icon-theme.folders.theme": "specific",
  "material-icon-theme.folders.associations": {
    "ltw": "examples",
    "proj": "git",
    "labs": "test",
    "p2p": "routes",
    "maps": "environment",
    "graph": "cluster",
    "anim": "animation",
    "forms": "php",
    "sections": "layout",
    "theory": "functions",
    "scenes": "views",
    "slides": "syntax",
    "studio": "cluster",
    "actions": "Redux-actions",
    "reducers": "Redux-reducer",
    "workflows": "github",
    "midterms": "docs",
    "application": "app",
    "ajax": "json",
    "graphviewer": "fastlane",
    "primitives": "environment",
    "cmake-build-debug": "dist",
    "cmake-build-default": "dist",
    "Calculator": "functions",
    "bootstrap": "css",
    "partials": "template"
  },
  "material-icon-theme.files.associations": {
    "*.pickle": "database",
    "*.db": "database",
    "*.vert": "shaderlab",
    "*.frag": "shaderlab",
    "*.class": "drone",
    "dockerignore": "docker",
    ".env_production": "tune",
    "*.properties": "gradle",
    "artisan": "php",
    "*.jjt": "",
    "*.jj": "",
    "*.jjm": "",
    "*.env*": "tune",
    ".wslconfig": "tune",
    "*.sdp": "key",
    "*.jcfg": "tune",
    "*.mjpeg": "video"
  },
  "files.associations": {
    "*.pl": "prolog",
    "*.jmm": "java",
    "*.jj": "java",
    "*.jjt": "java",
    "*.j": "java",
    "*.blade.php": "blade",
    "*.in": "txt",
    "*.out": "txt",
    "*.input": "txt",
    "*.output": "txt",
    ".wslconfig": "environment",
    ".env*": "env"
  },
  "files.exclude": {
    "**/__pycache__": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true
  },
  "workbench.editorAssociations": {
    "*.png": "imagePreview.previewEditor",
    "*.pdf": "analyticsignal.preview-pdf"
  },

  /* @@@ Git Settings */
  "git.enabled": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.decorations.enabled": false,

  /* @@@ Terminal Settings */
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontWeight": "normal",
  "terminal.integrated.fontWeightBold": "400",
  "terminal.integrated.defaultProfile.windows": "Ubuntu-20.04 (WSL)",
  "terminal.integrated.allowChords": false,
  "terminal.integrated.drawBoldTextInBrightColors": false,

  /* @@@ General Settings */
  "zenMode.hideStatusBar": false,
  "atomKeymap.promptV3Features": true,
  "extensions.ignoreRecommendations": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "intelephense.environment.documentRoot": "",
  "intelephense.format.enable": true,
  "security.workspace.trust.untrustedFiles": "open",
  "vsintellicode.modify.editor.suggestSelection": "choseToUpdateConfiguration",
  "tabnine.experimentalAutoImports": true,
  "workbench.editor.untitled.hint": "hidden",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "git.ignoreRebaseWarning": true,
  "editor.inlineSuggest.enabled": true,
  "csharp.referencesCodeLens.enabled": false,
  "editor.renderWhitespace": "none",
  "blockman.n01LineHeight": 0,
  "editor.wordWrap": "on",
  "diffEditor.wordWrap": "on",
  "editor.guides.indentation": false,
  "editor.guides.bracketPairs": false,
  "python.formatting.provider": "black",
  "[robotframework]": {
    "editor.defaultFormatter": "d-biehl.robotcode"
  },
  "[bibtex]": {
    "editor.defaultFormatter": "James-Yu.latex-workshop"
  },
  "editor.accessibilitySupport": "off",
  "python.formatting.blackArgs": ["--line-length 100"],
  "[latex]": {
    "editor.defaultFormatter": "James-Yu.latex-workshop"
  },
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": true
},
"window.zoomLevel": 1.25
}
