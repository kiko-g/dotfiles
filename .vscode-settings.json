// -------------------------- GENERAL SETTINGS --------------------------
"window.zoomLevel": -1,
"explorer.sortOrder": "type",
"zenMode.hideStatusBar": false,
"C_Cpp.updateChannel": "Insiders",
"C_Cpp.enhancedColorization": "Enabled",
"atomKeymap.promptV3Features": true,
"git.confirmSync": false,
"liveServer.settings.CustomBrowser": "firefox",

// ---------------------- GENERAL EDITOR SETTINGS -----------------------
"editor.fontSize": 15,
"editor.quickSuggestionsDelay": 20,
"terminal.integrated.fontSize": 16,
"editor.cursorBlinking": "phase",
"editor.suggestSelection": "first",
"editor.multiCursorModifier": "ctrlCmd",
"editor.formatOnPaste": true,
"editor.fontLigatures": false,
"editor.minimap.enabled": false,
"editor.renderIndentGuides": false,
"editor.overviewRulerBorder": false,
"editor.cursorSmoothCaretAnimation": true,
"editor.fontFamily": "JetBrains Mono, Consolas",

"workbench.iconTheme": "material-icon-theme",
"workbench.tree.renderIndentGuides": "none",
"workbench.startupEditor": "newUntitledFile",
"workbench.tree.indent": 5,

// ------------------------ MATERIAL ICON THEME ------------------------
"material-icon-theme.saturation": 1,
"material-icon-theme.folders.color": "#dcf7fe",
"material-icon-theme.folders.theme": "specific",
"material-icon-theme.folders.associations": {
    "labs": "Test",
    "proj": "Git",
    "scenes": "Views",
    "projects": "",
    "maps": "Messages",
    "graph": "Cluster",
    "anim": "Animation",
    "application": "App",
    "theory": "Functions",
    "graphviewer": "Fastlane",
    "primitives": "Environment",
    "cmake-build-debug": "Dist",
    "cmake-build-default": "Dist",
},
"material-icon-theme.files.associations": {
    "*.vert": "shaderlab",
    "*.frag": "shaderlab",
    "*.db": "database",
},
"files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
},

// -------------------------------- PROLOG --------------------------------
"files.associations": {
    "*.pl": "prolog"
},

// --------------------------------- DART ---------------------------------
"dart.closingLabels": true,
"dart.sdkPath": "C:\\Program Files\\Dart\\dart-sdk\\bin\\dart.exe",

// --------------------------------- GIT  ---------------------------------
"git.enabled": true,
"git.enableSmartCommit": true,
"git.decorations.enabled": false,
"[javascript]": {
